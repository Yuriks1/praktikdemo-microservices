<beanio xmlns="http://www.beanio.org/2012/03"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">

    <stream name="inboundCurrencyStream" format="xml">
        <record name="inboundCurrencyExchange" class="se.replyto.microservices.xmluppgift.beans.InboundCurrencyExchange">
            <field name="id" minOccurs="1" />
            <field name="from" minOccurs="1" />
            <field name="to" minOccurs="1" />
            <field name="conversionMultiple" minOccurs="1" />
        </record>
    </stream>
</beanio>